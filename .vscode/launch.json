    "version": "0.2.0",
    "configurations": [
        {
            "name": "Streamlit UI",
            "consoleTitle": "Streamlit UI",
            "type": "debugpy",
            "request": "launch",
            "module": "streamlit",
            "args": [
                "run",
                "hbit/ui/security_agent.py",
                "--server.fileWatcherType=auto",
                "--server.runOnSave=true",
            ],
            "cwd": "${workspaceFolder}",
            "jinja": true,
            "envFile": "${workspaceFolder}/.env",
            "justMyCode": false,
        },
        {
            "name": "Agent Device Evaluator",
            "consoleTitle": "Agent Device Evaluator",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceRoot}/hbit/cli.py",
            "env": {
                "PYTHONPATH": "."
            },
            "args": [
                "get_agent_evaluation",
                // "How security is my iPhone 13 Pro device if I have patch 18.1.0 installed identified by build 22B83."
                "Write a summary of how secure is an iPhone XS running iOS 17.7.2? Are there any known vulnerabilities or security concerns with this version?"
            ],
            "justMyCode": false,
            "envFile": "${workspaceFolder}/.env",
        },
        {
            "name": "Chain Device Evaluator",
            "consoleTitle": "Chain Device Evaluator",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceRoot}/hbit/cli.py",
            "env": {
                "PYTHONPATH": "."
            },
            "args": [
                "get_chain_evaluation",
                "How security is my iPhone 13 Pro device if I have patch 18.1.0 installed identified by build 22B83."
            ],
            "justMyCode": false,
            "envFile": "${workspaceFolder}/.env",
        },
        {
            "name": "Structured Summarization Evaluator",
            "consoleTitle": "Structured Summarization Evaluator",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceRoot}/hbit/cli.py",
            "env": {
                "PYTHONPATH": "."
            },
            "args": [
                "get_structured_evaluation",
                // "How security is my iPhone 13 Pro device if I have patch 18.1.0 installed identified by build 22B83."
                "How secure is an iPhone XS running iOS 17.7.2? Are there any known vulnerabilities or security concerns with this version?"
            ],
            "justMyCode": false,
            "envFile": "${workspaceFolder}/.env",
        },
        {
            "name": "SQL Summarization Evaluator",
            "consoleTitle": "SQL Summarization Evaluator",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceRoot}/hbit/cli.py",
            "env": {
                "PYTHONPATH": "."
            },
            "args": [
                "get_sql_evaluation",
                // "How security is my iPhone 13 Pro device if I have patch 18.1.0 installed identified by build 22B83."
                "How secure is an iPhone XS running iOS 17.7.2? Are there any known vulnerabilities or security concerns with this version?"
            ],
            "justMyCode": false,
            "envFile": "${workspaceFolder}/.env",
        },
        {
            "name": "Debug FastAPI",
            "consoleTitle": "Debug FastAPI",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "hbit_api.main:app",
                "--reload"
            ],
            "cwd": "${workspaceFolder}",
            "jinja": true,
            "envFile": "${workspaceFolder}/.env",
            "justMyCode": false,
        },
        {
            "name": "Debug PyTest",
            "consoleTitle": "Debug PyTest",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "args": [
                "hbit_api/tests"
            ],
            "cwd": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/.env",
            "justMyCode": false,
        },
        {
            "name": "FastAPI Healthy",
            "consoleTitle": "FastAPI Healthy",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/hbit_api/cli.py",
            "args": [
                "healthy"
            ],
            "cwd": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/.env",
            "env": {
                "PYTHONPATH": "."
            },
            "justMyCode": false,
        },
        {
            "name": "Migrate",
            "consoleTitle": "Migrate",
            "type": "debugpy",
            "request": "launch",
            "module": "alembic",
            "env": {
                "PYTHONPATH": "."
            },
            "args": [
